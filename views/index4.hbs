<!DOCTYPE HTML>
<html>
	<head>
		<title>Delete Operation</title>
	</head>
	<body>
		<div>
			<h1>Delete Operation</h1>
		</div>
		<div>
			{{#each sendData}}
			<h3>ID: {{this._id}}</h3>
			<h5>Name:</h5>
			<p>{{this.name}}</p>
			<h5>email:</h5>
			<p>{{this.email}}</p>
			<form action="/delete-user/{{this.name}}/{{this.email}}" method="POST">
				<input type="hidden" name="id" value="{{this._id}}">
				<input type="submit" value="Delete">
			</form>
			{{/each}}
		</div>
	</body>
</html>